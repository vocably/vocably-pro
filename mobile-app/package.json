{
  "name": "VocablyPro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "reset-ios": "rm -rf ios/build ios/Pods && npx pod-install",
    "deploy-ios": "(cd ios && fastlane ios build && fastlane ios beta)",
    "deploy-android": "(cd android && fastlane android beta)",
    "mobile-app": "npm start",
    "list-devices": "xcrun simctl list devices"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^9.0.0",
    "@react-navigation/drawer": "^6.5.6",
    "@react-navigation/material-bottom-tabs": "^6.2.14",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.2.0",
    "@shopify/flash-list": "^1.4.0",
    "amazon-cognito-identity-js": "^6.0.1",
    "aws-amplify": "^5.0.5",
    "aws-amplify-react-native": "^7.0.2",
    "react": "18.2.0",
    "react-native": "^0.72.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-paper": "^5.1.2",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": ">=4.5.3 <4.8",
    "react-native-screens": ">=3.21.0 <3.28.0",
    "react-native-sound": "^0.11.2",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.5",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.3",
    "@types/jest": "^29.2.1",
    "@types/react-native": "^0.70.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "lodash-es": "^4.17.21",
    "react-native-dotenv": "^3.4.8"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=16"
  }
}
